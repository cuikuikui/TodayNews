 <view class="personal_info">
	<view class="photo_wrap">
		<image src="{{userInfo.avatarUrl}}" class="photo"></image>
	</view>
	<view class="nickname">
		{{userInfo.nickName}}
	</view>
</view>
<view class="wode_item_wrap">


    <view class="wode_item">
		我的收藏：{{gloabalData}}
		<span class="arrow_wrap"></span>
	</view>

	<view class="wode_item" wx:for="{{mode}}" wx:key="id">
		{{item}}
		<span class="arrow_wrap"></span>
	</view>
</view>
<view class="wode_out" bindtap="exit">退出</view>
 



